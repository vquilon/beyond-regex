{
    "name": "@lezer/regex",
    "version": "0.0.1",
    "description": "Lezer-based RegEx grammar",
    "main": "dist/index.cjs",
    "type": "module",
    "exports": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs"
    },
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "author": "Victor Quilon Ranera <v.quilonr@gmail.com>",
    "license": "MIT",
    "devDependencies": {
      "@lezer/generator": "^1.0.0",
      "mocha": "^9.0.1",
      "rollup": "^2.52.2",
      "@rollup/plugin-node-resolve": "^9.0.0"
    },
    "dependencies": {
      "@lezer/lr": "^1.0.0",
      "@lezer/highlight": "^1.0.0"
    },
    "repository": {
      "type" : "git",
      "url" : "https://github.com/lezer-parser/python.git"
    },
    "scripts": {
      "build": "lezer-generator src/regex.grammar -o src/parser && rollup -c",
      "build-debug": "lezer-generator src/regex.grammar --names -o src/parser && rollup -c",
      "prepare": "npm run build",
      "test": "mocha test/test-*.js"
    }
  }